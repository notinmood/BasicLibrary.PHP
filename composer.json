{
  "name": "hiland/basiclibrary.php",
  "description": "RainyTop & Hiland 's php basic library.",
  "homepage": "http://www.rainytop.com/",
  "keywords": [
    "php",
    "basic library",
    "codecs"
  ],
  "type": "string",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.0",
    "ext-mbstring": "*",
    "topthink/think-orm": "^3.0",
    "symfony/options-resolver": "^5.4",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-bcmath": "*",
    "ext-iconv": "*",
    "ext-gd": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*"
  },
  "autoload": {
    "psr-4": {
      "Hiland\\Test\\": "tests",
      "Hiland\\": "src"
    },
    "files": [
      "src/Common/functions.php"
    ]
  }
}
